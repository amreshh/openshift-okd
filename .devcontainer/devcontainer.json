{
    "dockerFile": "Dockerfile",
    "shutdownAction": "stopContainer",
    "postStartCommand": "cp /workspaces/openshift-okd/install-config.yaml /workspaces/openshift-okd/ignition_configs/install-config.yaml",
    "mounts": [
      "source=/var/run/libvirt/virtqemud-sock,target=/var/run/libvirt/libvirt-sock,type=bind,consistency=cached"
    ],
    "customizations": {
      "vscode": {
        "extensions": [
          "VisualStudioExptTeam.vscodeintellicode",
          "GitHub.copilot",
          "GitHub.copilot-chat",
          "bierner.markdown-mermaid",
          "docsmsft.docs-markdown",
          "hediet.vscode-drawio",
          "HashiCorp.terraform",
          "eamodio.gitlens"
        ]
      }
    }
  }